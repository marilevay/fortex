{
    "name": "ForTex",
    "description": "Create LaTeX formulas on a Quill text editor.",
    "version": "2.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "fortex.html",
        "default_icon": "fortex-icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://forum.minerva.edu/*", "https://sle-collaboration.minervaproject.com/*", "https://class.minerva.edu/classes/*", "https://marilevay.github.io/forum-tex/demo/*"],
            "js": ["quillOverride.js"],
            "run_at": "document_idle",
            "all_frames": true
        }
    ]
}