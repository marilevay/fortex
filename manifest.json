{
    "name": "ForumTex",
    "description": "Create LaTeX formulas on Forum",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "forum-tex.html",
        "default_icon": "forum-tex-icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://forum.minerva.edu/*", "https://sle-collaboration.minervaproject.com/*", "https://class.minerva.edu/classes/*"],
            "js": ["quillOverride.js"],
            "run_at": "document_idle",
            "all_frames": true
        }
    ]
}